(q2)
public class basic2
{
    public static void main(String args[])
    {
        int a=125;
        int b=24;int k=a%b;
        System.out.printf("\n125 + 24 =%d\n",a+b); 
        System.out.printf("125 - 24 = %d\n",a-b);
        System.out.printf("125 x 24 = %d\n",a*b);
        System.out.printf("125 / 24 = %d\n",a/b);
        System.out.printf("125 mod 24 = %d",k);
    }
}
(q3)
import java.util.*;
public class basic3
{
    public static void main(String args[])
    {
        Scanner in = new Scanner(System.in);
        System.out.println("\nEnter a number\n");
        int num= in.nextInt();
        for(int i=1;i<=10;i++)
        {
            System.out.printf(num+"x"+i+"=%d\n",(num*i));
        }
    }
}
